array=[2,30,4959,56];
let check_prime=()=>
{
 for(i=0;i<array.length;i++)
    {
        let count=0;
        n=array[i];
        for(j=1;j<=n;j++)
        {
           if(n%j===0)
           {
               count++;
           }
        }
        if(count==2)
        {
            console.log("Prime",array[i]);
        }
        
        else{
            
          console.log("Not Prime",array[i]);
        }
    }
    
};
check_prime();
