//Anonyms Function 

let str=function(arr)
{
    for(i=0;i<arr.length;i++)
    {
       if(arr[i]%2!==0)
       {
           console.log(arr[i]);
       }
    }
};
str([21,30,40,50,60]);


//IIFE

(function(arr){
    for(i=0;i<arr.length;i++)
    {
       if(arr[i]%2!==0)
       {
           console.log(arr[i]);
       }
    }
    
})([21,30,40,50,60]);
